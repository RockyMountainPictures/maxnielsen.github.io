step=\"0.1\" value=\"0.8\" condition=\"overlay && bg.type !== 'color'\">\n        <!-- End block parameters -->\n    </mbr-parameters>\n\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type!== 'color'\" mbr-style=\"{'opacity': overlayOpacity, 'background-color': overlayColor}\">\n    </div>\n    <div mbr-class=\"{'container-fluid': fullWidth, 'container': !fullWidth }\">\n        <div class=\"row\">\n            <div class=\"title col-12 align-center\" mbr-if=\"showTitle || showSubtitle\">\n                <div class=\"mbr-section-btn align-right awesome2 awesome3\" mbr-buttons mbr-theme-style=\"display-4\"><a class=\"awesome4 awesome2 btn btn-md btn-white-outline\" href=\"index.html#features06-19\" data-app-placeholder=\"Type Text\"><span class=\"awesome2 icon54-v1-down-left1 mbr-iconfont mbr-iconfont-btn\"></span>Back</a></div>\n                <h2 class=\"pb-5 mbr-light mbr-fonts-style mbr-white\" mbr-theme-style=\"display-1\" mbr-if=\"showTitle\"><b>Renovation (2024)</b></h2>\n            </div>\n            <div class=\"col-12 col-md-3 lefttitle\">\n                <div class=\"separator d-flex flex-row flex-nowrap\" mbr-if=\"showtitle\">\n                    <span class=\"sep-holder w-100\">\n                        <span class=\"sep-line d-block w-100\"></span>\n                    </span>\n                </div>\n                <p class=\"align-left mbr-fonts-style text-box mbr-white\" data-app-selector=\".text2, .text-box\" mbr-theme-style=\"display-5\" mbr-if=\"showtitle\">2024<br><a href=\"https://www.revitalizeyourhome.com/\" class=\"text-success\" target=\"_blank\">Revitalize Home Repair &amp; Renovations</a><br>Commission<br><br><b>^&nbsp;Details</b></p>\n                <div class=\"separator d-flex flex-row flex-nowrap\" mbr-if=\"showtitle\">\n                    <span class=\"sep-holder w-100\">\n                        <span class=\"sep-line d-block w-100\"></span>\n                    </span>\n                </div>\n            </div>\n  